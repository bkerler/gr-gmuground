<?xml version="1.0"?>
<block>
  <name>Square Root</name>
  <key>gmuground_sqrt</key>
  <category>[gmuground]</category>
  <import>import gmuground</import>
  <make>gmuground.sqrt()</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <!-- <param>
    <name>in</name>
    <key>complex</key>
    <type>1</type>
  </param> -->

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>1</vlen>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>1</vlen>
  </source>
</block>
